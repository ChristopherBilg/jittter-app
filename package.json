{
  "name": "jittter-app",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual -c \"npm run start\"",
    "format:check": "prettier --check app/",
    "format:write": "prettier --write app/",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler pages dev --compatibility-date=2023-12-01 ./public",
    "drizzle:generate": "drizzle-kit generate:pg",
    "drizzle:push": "drizzle-kit push:pg",
    "drizzle:studio": "drizzle-kit studio",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.8.1",
    "@remix-run/cloudflare": "^2.5.1",
    "@remix-run/cloudflare-pages": "^2.5.1",
    "@remix-run/css-bundle": "^2.5.1",
    "@remix-run/react": "^2.5.1",
    "drizzle-orm": "^0.29.3",
    "isbot": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240129.0",
    "@remix-run/dev": "^2.5.1",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "drizzle-kit": "^0.20.13",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "pg": "^8.11.3",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.1.0",
    "wrangler": "^3.28.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
